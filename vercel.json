{
  "version": 2,
  "env": {
    "FLASK_ENV": "production"
  },
  "functions": {
    "api/index.py": {
      "runtime": "python3.12"
    }
  },
  "routes": [
    {
      "src": "^/static/(.*)$",
      "dest": "/frontend/static/$1"
    },
    {
      "src": "^/(.*?)/?$",
      "dest": "/api/index.py"
    }
  ]
}
