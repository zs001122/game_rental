{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  },
  "routes": [
    {
      "src": "^/static/(.*)$",
      "dest": "/frontend/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}
